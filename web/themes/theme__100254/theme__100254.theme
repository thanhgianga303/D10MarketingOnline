/* set alias for all the menu links */
function theme__100254_preprocess_html(&$variables) {
	$current_path = \Drupal::service('path.current')->getPath();
	if (((int) \Drupal::VERSION) < 9)
  {
	$path_alias = \Drupal::service('path.alias_manager')->getAliasByPath($current_path);
	}
	else
	{
		$path_alias = \Drupal::service('path_alias.manager')->getAliasByPath($current_path);
	}
	if ($path_alias == $current_path)
	{
		$path_alias = 'blog';
	}
	else
	{
		$path_alias = ltrim($path_alias, '/');
	}
/* unset main-menu from primary menu position */
	$variables['attributes']['class'][] = \Drupal\Component\Utility\Html::cleanCssIdentifier($path_alias);
	if (isset($variables['page']['primary_menu']['theme__100254_main_menu'])) 
	{
		unset($variables['page']['primary_menu']['theme__100254_main_menu']);
	}
}
