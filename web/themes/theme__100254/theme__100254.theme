/* set page layout file for each node */
function theme__100254_theme_suggestions_page_alter(&$suggestions, &$vars)
{
$alias = '';
	\Drupal::configFactory()->getEditable('theme__100254.settings')->set('is_content_imported', '0')->save();
	$frontPage = \Drupal::service('path.matcher')->isFrontPage();
	if (!$frontPage)
	{
		$node = \Drupal::request()->attributes->get('node');
		if ($node == null){
		$alias = 'blog';
		}
		else{
			if (((int) \Drupal::VERSION) < 9)
			{
		$alias = \Drupal::service('path.alias_manager')->getAliasByPath('/node/'.$node->id());
		}
			else
			{
				$alias = \Drupal::service('path_alias.manager')->getAliasByPath('/node/'.$node->id());
			}
		}
		}
		$tt_nodes = array(
		'home', 
		);
 		if (in_array($alias,$tt_nodes) || $alias == 'blog')
  	{
			$suggestions[] = 'page--node';
		}
	}
/* Content import ends */
function theme__100254_theme_suggestions_block_alter(array &$suggestions, array $variables) {
		$base_plugin_id = $variables['elements']['#base_plugin_id'];
		$system_menu_block = $base_plugin_id == 'system_menu_block';
		if ($system_menu_block) {
		 $id = $variables['elements']['#id'];
		 $block = Block::load($id);
		 $region = $block->get('region');
		 $suggestions[] = 'block__' . $region . '__menu';
		 }
		} 
