/* Content import starts */
function theme__100254_add_content()
{
	$themeLink = drupal_get_path('theme', 'theme__100254');
	$xml = simplexml_load_file($themeLink.'/content/content.xml');
		$menu_arr = array();
		foreach($xml->menu->menu_item as $menu_name){
			if ((string) $menu_name->menu_item_slug == 'blog-wp') {
				continue;
			}
			array_push(
				$menu_arr,
				array(
					'title'  => (string) $menu_name->menu_item_title,
					'status' => (int) $menu_name->menu_item_status,
					'slug'   => (string) $menu_name->menu_item_slug,
				)
			);
		}
	if(isset($xml->pages->content))	
	{
	/* Copy all the content images to sites->default->files->inline-images */
		$fid = 10;
		if ( ((int) \Drupal::VERSION) < 9) {
		$file = file_load($fid); 
		}
		else {
			$file = \Drupal\file\Entity\File::load($fid); 
		}
      $path = 'public://inline-images/'; 
		if ( ((int) \Drupal::VERSION) < 9) {
		if (file_prepare_directory($path, FILE_CREATE_DIRECTORY) && !file_exists($path)) 
		{
	  		file_copy($file , $path);
		}
		$images = file_scan_directory($themeLink.'/content/images','/.*\.jpg|jpeg|gif|png|pdf|doc|zip|rar|docx|7zip|txt|rtf|odt|7z|ods$/');
		$videos = file_scan_directory($themeLink.'/video','/.*\.mp4$/');
		}
		else {
		if (\Drupal::service('file_system')->prepareDirectory($path, FileSystemInterface::CREATE_DIRECTORY) && !file_exists($path)) 
		{
	  		file_copy($file , $path);
		}
		$images_dir = $themeLink.'/content/images';
		if (is_dir($images_dir)) {
			$images = \Drupal::service('file_system')->scanDirectory($images_dir,'/.*\.jpg|jpeg|gif|png|pdf|doc|zip|rar|docx|7zip|txt|rtf|odt|7z|ods$/');
		}
		$videos_dir = $themeLink.'/video';
		if (is_dir($videos_dir)) {
			$videos = \Drupal::service('file_system')->scanDirectory($videos_dir,'/.*\.mp4$/');
		}
		}
		foreach($images as $value)
		{
			$file = array(
			'uri'           => $value->uri,
